CREATE TABLE post (            id VARCHAR(10) NOT NULL, title VARCHAR(100) NOT NULL,            description VARCHAR(100) NOT NULL, creator_id VARCHAR(50) NOT NULL, status VARCHAR(10) NOT NULL , image VARCHAR(50), event_venue VARCHAR(100) , event_date VARCHAR(20) , event_capacity INT , event_attendee_count INT , sale_asking_price FLOAT , sale_highest_offer FLOAT , sale_minimum_raise FLOAT , job_proposed_price FLOAT ,             job_lowest_offer FLOAT , PRIMARY KEY (id))
